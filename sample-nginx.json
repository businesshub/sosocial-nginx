{
  "id": "sosocial-nginx",
  "instances": 1,
  "cpus": 0.25,
  "mem": 256,
  "uris": [],
  "container": {
      "type": "DOCKER",
      "docker": {
          "image": "molinto/ngnix",
          "network": "BRIDGE",
          "portMappings": [
            { "containerPort": 80, "hostPort": 0, "protocol": "tcp" }
          ]
      }
  },
  "healthChecks": [
      {
          "protocol": "HTTP",
          "portIndex": 0,
          "path": "/",
          "gracePeriodSeconds": 5,
          "intervalSeconds": 20,
          "maxConsecutiveFailures": 3
      }
  ],
  "cmd": ""
}